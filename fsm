	EMO [label="EMO\l"]
	chooseTime [label="chooseTime\l"]
	morning [label="morning\l"]
	afternoon [label="afternoon\l"]
	night [label="night\l"]
	reject1 [label="reject1\l"]
	reject2 [label="reject2\l"]
	user -> main [label="advance [is_going_to_main]"]
	happy -> main [label="advance [is_going_to_main]"]
	EMO -> main [label="advance [is_going_to_main]"]
	main -> chooseEmo [label="advance [is_going_to_chooseEmo]"]
	main -> chooseTime [label="advance [is_going_to_chooseTime]"]
	main -> reject1 [label="advance [is_going_to_reject1]"]
	chooseEmo -> happy [label="advance [is_going_to_happy]"]
	chooseEmo -> EMO [label="advance [is_going_to_EMO]"]
	chooseTime -> morning [label="advance [is_going_to_morning]"]
	chooseTime -> afternoon [label="advance [is_going_to_afternoon]"]
	chooseTime -> night [label="advance [is_going_to_night]"]
	morning -> main [label="advance [is_going_to_main]"]
	afternoon -> main [label="advance [is_going_to_main]"]
	night -> main [label="advance [is_going_to_main]"]
	reject1 -> main [label="advance [is_going_to_main]"]
	reject1 -> reject2 [label="advance [is_going_to_reject2]"]
	reject2 -> main [label="advance [is_going_to_main]"]
}
